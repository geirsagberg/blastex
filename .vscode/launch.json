  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'blastex'",
      "cargo": {
        "args": [
          "build",
          "--bin=blastex",
          "--package=blastex"
        ],
        "filter": {
          "name": "blastex",
          "kind": "bin"
        }
      },
      "env": {
        "CARGO_MANIFEST_DIR": "${workspaceFolder}"
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'blastex'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=blastex",
          "--package=blastex"
        ],
        "filter": {
          "name": "blastex",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}